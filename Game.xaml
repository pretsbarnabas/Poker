<Page x:Class="Poker.Game"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Poker"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Game" Background="White">
      <!--xmlns:fx="https://unknown6656.com/wpfpslib"-->

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Rectangle Width="70" Height="70" Fill="Red" Grid.Column="0"></Rectangle>
        <Rectangle Width="70" Height="70" Fill="Green" Grid.Column="1"></Rectangle>
        <Rectangle Width="70" Height="70" Fill="Blue" Grid.Column="2"></Rectangle>
        <Rectangle Width="70" Height="70" Fill="Yellow" Grid.Column="3"></Rectangle>
        <Rectangle Width="70" Height="70" Fill="HotPink" Grid.Column="4"></Rectangle>
    </Grid>
</Page>
